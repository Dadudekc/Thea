# ChatGPT Credentials
# Copy this file to .env and fill in your actual credentials
CHATGPT_USERNAME=your-email@example.com
CHATGPT_PASSWORD=your-password

# Optional: TOTP Secret for 2FA (if you have 2FA enabled)
# Get this from your authenticator app setup
CHATGPT_TOTP_SECRET=your-totp-secret-here

# Optional: Custom cookie file path
CHATGPT_COOKIE_FILE=chatgpt_cookies.pkl

# Optional: Scraper settings
# CHATGPT_HEADLESS=false
# CHATGPT_TIMEOUT=30
# CHATGPT_USE_UNDETECTED=true

# Optional: Logging settings
# DREAMSCAPE_LOG_LEVEL=INFO
# DREAMSCAPE_DEBUG=false

# Tools Database (optional)
# Location of the SQLite file that stores CLI tools
TOOLS_DB_PATH=data/tools.db

# Discord Bot Configuration (Phase 3)
# -------------------------------------------------
# The Discord bot integration becomes active in Phase 3.  Copy these keys
# into your local .env file and fill them with values from the Discord
# Developer Portal (https://discord.com/developers/applications).
# NOTE: Leave the fields blank to keep the bot disabled.
DISCORD_BOT_TOKEN=
DISCORD_APPLICATION_ID=
DISCORD_CLIENT_ID=
DISCORD_GUILD_ID=
DISCORD_CHANNEL_ID= (this will lead to general channel)

# Multiple Discord Channels
# -------------------------
# Map separate channel IDs for specialized updates.
# (Leave blank to disable that channel.)
DISCORD_CHANNEL_DEVLOG_ID=
DISCORD_CHANNEL_MMRPG_ID=
# Agent-specific channels
DISCORD_CHANNEL_AGENT_1=
DISCORD_CHANNEL_AGENT_2=
DISCORD_CHANNEL_AGENT_3=
DISCORD_CHANNEL_AGENT_4=
# MMORPG event channels
DISCORD_CHANNEL_SKILL_ID=
DISCORD_CHANNEL_QUESTS_ID=
DISCORD_CHANNEL_EQUIPMENT_ID=
DISCORD_CHANNEL_LORE_ID=
DISCORD_CHANNEL_STORY_ID=

